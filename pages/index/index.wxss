  margin-top: 30rpx;
  width: 94rpx;
  height: 24rpx;
}
.is_show{
  display: none;
}

textarea{
  width: 100%;
  height: 160rpx;
  padding: 20rpx 30rpx;
  background: #FAFAFA;
}

checkbox{
