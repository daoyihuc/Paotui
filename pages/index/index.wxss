.submit-right{
  width: 250rpx !important;
  color: #fff;
  border-top-right-radius: 999rpx;
  border-bottom-right-radius: 999rpx;
}

.shadow {
  -webkit-box-shadow: 0px -2px 4px rgba(253, 133, 73, 0.4), 0 0 6px rgba(0, 0, 0, 0.04);
  box-shadow: 0px -2px 4px rgba(253, 133, 73, 0.4), 0 0 6px rgba(0, 0, 0, 0.04);
}

.meu-cell__bd{
  display: flex;
  align-items: center;
}

.icon{
  width: 30rpx;
  height: 30rpx;
  margin-right: 15rpx;
}

.meu-cell{
  padding: 40rpx 30rpx;
}

.logo{
  width: 46rpx !important;
  height: 46rpx !important;
}
/* 取件码 */
.input_codebox{
  display: flex;
  flex-direction: row;
  background: white;
  align-items: center;
}
.input_codebox .add{
  display: flex;
  flex-direction: row;
  background: white;
  align-items: center;
  color: #FE924C;
  font-size: 11px;
}
.input_codebox image{
  width: 40rpx;
  height: 40rpx;
  margin: 20rpx 10rpx;
}
.input_code{
  background: #E5E5E5;
  line-height: 77rpx;
  height: 77rpx;
  margin-top: 8rpx;
  margin-left: 20rpx;
  padding-left: 8px;
  margin-bottom: 8px;
  width: 500rpx;

}